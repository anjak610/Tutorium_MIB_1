
public class Nullstellen {
	
	public static void main (String[] args)
	{
		double x = 0;
		double x2 = 0;
		//double z = 0;
		f(-1,20);
		//nullstelle(-1,20,0.0001);
		
	}
	
	public static double f(double x, double x2)
	{
		
		//System.out.println(x+y);
		double m = Math.exp(x2)+(x2*x2)-4;
		System.out.println(m);
		return m;
		
	}
	
	public static double nullstelle (double x, double y, double z)
	{
		double z = 0.0001;
		double mitte = (x+y)/2;
		
		if(Math.abs(x-y)< z)
		{
			return mitte;
		}
		//else if ()
		
		return nullstelle(x,y,z);
		
	}

}
