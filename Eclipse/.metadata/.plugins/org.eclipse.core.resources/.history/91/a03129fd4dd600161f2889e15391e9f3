package klasuraufgabgen_1;

public class B {
	
	public static int x[] = {2,1,0,2,3};
	public static String y[] = {"das" , "18", "Auto", "grüm"};
	
	public static void main(String args[])
	{
		try{
			for(int i = 0; i<7; i++)
			{
				System.out.println("i= " +i);
				System.out.println("y[x[i]] = " + y[x[i]]);
			}
		}
	}

}

