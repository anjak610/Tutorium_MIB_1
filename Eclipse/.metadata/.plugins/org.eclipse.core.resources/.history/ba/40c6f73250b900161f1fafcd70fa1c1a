package Aufgaben_Eisenbiegler;

public class Iterative_Fibonacci {
	
	public static void main (String[] args)
	{
		int n = 7;
		System.out.println(fib(n));
	}
	
	public static int fib(int n)
	{
		int a = 0;
		int b = 1;
		
		for (int i =0; i < n; i++)
		{
			a = a+b;
			b = a;
			
		}
		return a;
	}

}
