package Examples.ObjectMethods;

public class Points {
	
	int x_coord;
	int y_coord;


	 public void Point ()
	{
		x_coord = 10;
		y_coord = 10;
	}
	
	public void Point2 ( int _newXcoord)
	{
		x_coord = _newXcoord;
		y_coord = 10;
	}
	
	public void Point3 ( int _newXcoord,  int _newYcoord)
	{
		x_coord = _newXcoord;
		y_coord = _newYcoord;
		
	}
	
	
}
