package Examples;

public class ReturnValues {
	
	public static void main (String[]args)
	{
		int x = 9;
		int y = 3;
		add(x,y);
	}
	
	public static void add(int x, int y) // Methode gibt nichts zurück
	{
		int z = x+y;
		System.out.println(z);		
	}
	
	// Wenn man eine z.B int Variable zurückgeben will, dann schreibt man statt void int
	
	public static int add2 (int x, int y)
	{
		
	}

}
