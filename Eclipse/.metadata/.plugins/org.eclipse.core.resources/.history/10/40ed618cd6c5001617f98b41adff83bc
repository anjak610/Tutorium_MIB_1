package Aufgabe14_6;

public class Primzahlen {


	
	public static void main (String[]args)
	{
		int a = 12;
		int b = 5;
		int c = 2351;
		System.out.println(teilbar(a,b));
		System.out.println(prim(c));
		
	}
	public static boolean teilbar(int a, int b)
	{
		if(a%b == 0)
		{
			return true;
		}
		return false;
	}
	
	public static boolean prim(int c)
	{
		for (int i = 2; i < c; i++)
		{
			if (c%i == 0)
			{
				return false;
			}		
		}
		return true;
	}
	
}
