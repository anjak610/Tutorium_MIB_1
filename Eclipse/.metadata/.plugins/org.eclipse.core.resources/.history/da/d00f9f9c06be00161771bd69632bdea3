package Aufgabe_10;

public class StackTest {
	
	  public static void main(String[] args) {
	      MyStack theStack = new MyStack(10); 
	      theStack.push("Hund");
	      theStack.push("Katze");
	      theStack.push(30);
	      theStack.push(40);
	      theStack.push(50);
	      theStack.pop();
	      //theStack.pop();
	      while (!theStack.isEmpty()) {
	         Object value = theStack.pop();
	         System.out.print(value.toString());
	         System.out.print(" ");
	      }
	      System.out.println("");
	   }

}
