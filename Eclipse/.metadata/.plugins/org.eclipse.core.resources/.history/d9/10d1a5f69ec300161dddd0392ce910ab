package Aufgabe14_5;

public class Quadrat {


	public static void main(String[] args) {
	
		int k = 6;
		quadrate(k);
	    String result = "";
	}

	private static void quadrate(int k) {


		if (k > 0 ) {	
			System.out.print("*");			
			quadrate(k - 1);	
		}

		if (k > 1) {
			System.out.print("\n"+"*");		
			System.out.print("-");//+ "-"	
			
		}
		
		
		
		
	}
}
