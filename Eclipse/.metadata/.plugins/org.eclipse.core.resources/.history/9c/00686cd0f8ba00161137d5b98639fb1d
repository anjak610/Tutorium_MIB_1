package Übung_Vererbung;

public class House {
	
	// the Bicycle class has three fields
	public int floors;
	public int rooms;
	public String color;
	
	// the Bicycle class has one constructor --> neuer Konstruktur gebaut 
	public House(int roomNumbers, int floorNumbers, String originalColor)
	{
		color = originalColor;
		rooms = roomNumbers;
		floors = floorNumbers;		
	}
	
