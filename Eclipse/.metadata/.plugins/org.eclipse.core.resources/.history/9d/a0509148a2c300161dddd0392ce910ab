package Aufgabe14_5;

public class Quadrat {

	public static String result = "";

	public static void main(String[] args) {
	
		int k = 6;
		quadrate(k);
	    
	}

	private static void quadrate(int k) {

	
		if (k > 0 ) {	
			System.out.print("*");	
			result += "-";
			quadrate(k - 1);
			
			if(k>=1)
			{
				//result += "-";
			 System.out.println("*" + result-1);//+ "-"
			}		
					
		}

		
		
	}
}
