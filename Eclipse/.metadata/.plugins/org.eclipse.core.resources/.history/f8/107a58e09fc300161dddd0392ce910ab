package Aufgabe14_5;

public class Quadrat {

	public static String result = "";

	public static void main(String[] args) {
	
		int k = 6;
		quadrate(k);
	    
	}

	private static void quadrate(int k) {

		for (int i = 0; i<=k; i++)
		{
		    
			result += "*";			
		}
		System.out.print(result);
		/*if (k > 0 ) {	
			System.out.print("*");			
			quadrate(k - 1);	
		}

		if (k > 1) {
			System.out.print("\n"+"*");		
			System.out.print("-");//+ "-"	
			
		}*/
		
		
		
		
	}
}
