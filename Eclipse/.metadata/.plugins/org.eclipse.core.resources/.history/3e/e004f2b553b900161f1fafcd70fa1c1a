package Aufgaben_Eisenbiegler;

public class Iterative_Fibonacci {
	
	public static void main (String[] args)
	{
		int n = 5;
		System.out.println(fib(n));
	}
	
	public static int fib(int n)
	{
		int fib = 0;
		int a = 1;
		//int fib2 = fib+a; 
		
		for (int i =0; i < n; i++)
		{
			fib = fib+a;
			a = fib;
			System.out.println(fib);
			
		}
		return fib;
	}

}
