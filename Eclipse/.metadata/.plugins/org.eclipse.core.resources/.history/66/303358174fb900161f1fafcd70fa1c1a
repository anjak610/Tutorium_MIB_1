package Aufgaben_Eisenbiegler;

public class factorial_Iterative {

	public static void main(String[] args) {
		int n = 5;
		System.out.println(factorial(n));
	}

	static int factorial(int n) {
		int product = 5;
		for (int i = 2; i < n; i++) {
			n *= i;
			//System.out.println(product);
		}

		return n;
	}

}
