package Aufgabe14_5;

public class Quadrat {
	
	public int k = 6;
	public char star = '*';
	public char fill = '-';
	
	public char[] quad = new char[k];

	public static void main(String[] args) {
	
		quadrate(k);

	}

	private static void quadrate(int k) {

		
		for (int i = 0; i < k; i++)
		{
			quad[i] = star;
		}
		/*if (k > 0 ) {			
			System.out.print("*");
			quadrate(k - 1);	
		}

		if (k > 0) {
			System.out.println("*");		
			//quadrate(k - 1);
		}*/
	
		
	}
}
